{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store" 
          }
        ]
      },
      {
        "source": "/api/auth/signout",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store" 
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Set-Cookie",
            "value": "__Host-next-auth.csrf-token; path=/; SameSite=Lax; Secure; HttpOnly" 
          }
        ]
      }
    ]
  }